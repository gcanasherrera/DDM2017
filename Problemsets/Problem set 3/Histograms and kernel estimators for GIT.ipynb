   },
   "outputs": [],
   "source": [
    "# Create  the grid on which to evaluate the results\n",
    "Nx = 50\n",
    "Ny = 125\n",
    "xmin, xmax = (-375, -175)\n",
    "ymin, ymax = (-300, 200)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "xgrid = np.linspace(xmin, xmax, Nx)\n",
    "ygrid = np.linspace(ymin, ymax, Ny)\n",
    "mesh = np.meshgrid(xgrid, ygrid)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "tmp = map(np.ravel, mesh)\n",
    "Xgrid = np.vstack(tmp).T"
   ]
  },
